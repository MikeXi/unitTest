<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="My Suite" parallel="methods" thread-count="2">
    <test name = "Calculator">
        <groups>
            <run>
                <include name="double"/>
                <include name="boolean"/>
                <include name="long"/>
            </run>
        </groups>
        <classes>
            <class name="com.epam.tat.module4.testng.tests.CosTest"/>
            <class name="com.epam.tat.module4.testng.tests.CtgTest"/>
            <class name="com.epam.tat.module4.testng.tests.DoubleDivTest"/>
            <class name="com.epam.tat.module4.testng.tests.DoubleMultTest"/>
            <class name="com.epam.tat.module4.testng.tests.DoubleSubTest"/>
            <class name="com.epam.tat.module4.testng.tests.DoubleSumTest"/>
            <class name="com.epam.tat.module4.testng.tests.IsNegativeTest"/>
            <class name="com.epam.tat.module4.testng.tests.IsPositiveTest"/>
            <class name="com.epam.tat.module4.testng.tests.LongDivTest"/>
            <class name="com.epam.tat.module4.testng.tests.LongMultTest"/>
            <class name="com.epam.tat.module4.testng.tests.LongSubTest"/>
            <class name="com.epam.tat.module4.testng.tests.LongSumTest"/>
            <class name="com.epam.tat.module4.testng.tests.PowTest"/>
            <class name="com.epam.tat.module4.testng.tests.SinTest"/>
            <class name="com.epam.tat.module4.testng.tests.SqrtTest"/>
            <class name="com.epam.tat.module4.testng.tests.TgTest"/>
        </classes>
    </test>
</suite>